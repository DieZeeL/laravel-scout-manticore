{
  "name": "diezeel/laravel-scout-manticore",
  "description": "Laravel Scout Manticore Driver",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.3",
    "laravel/scout": "^7.0|^8.0",
    "evilfreelancer/laravel-manticoresearch": "dev:master"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.2",
    "phpunit/phpunit": "^9.3.3",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Diezeel\\ManticoreScout\\ServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Diezeel\\ManticoreScout\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Diezeel\\ManticoreScout\\Tests\\": "tests/"
    }
  }
}
